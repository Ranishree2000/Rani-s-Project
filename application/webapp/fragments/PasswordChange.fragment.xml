<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
    xmlns:form="sap.ui.layout.form"
    

>
<Dialog id="_IDGenDialogpassPopup"
    contentHeight="300px"
    contentWidth="600px"
    title="CHANGE PASSWORD"
    titleAlignment="Center">

    <form:SimpleForm editable="true" id="SignupForm" width="600px"  class="sapUiTinyMargin" >
               
        <Label  text="EMP ID" class="FormLabels"/><Input id="EmpIdConfirm"  enabled="false"  width="240px" />
        <Label  text="Old Password" class="FormLabels"/><Input placeholder="old Password" required="true" liveChange="oldpass" type="Password"  id="oldPassword"  width="240px"/>

        <Label  text="Password" class="FormLabels"/><Input placeholder="Password" required="true" type="Password"  id="PasswordReset" liveChange="newpass" width="240px"/>
        
        <Label  text="Confirm password" class="FormLabels"/><Input placeholder="Confirm Password" required="true" type="Password" id="ConfirmPassword"  liveChange="ConfirmPassword" width="240px"/>
        <Label  />
    </form:SimpleForm>
    <Button text="Set Password" press="resetPassword"  class="setPassword" type="Emphasized"></Button>



<endButton>
<!-- <Button text="close" press="closeLogin" type="Emphasized"></Button> -->
</endButton>
</Dialog>
</c:FragmentDefinition>